{
  "name": "yiisoft/yii2-app-advanced",
  "description": "Yii 2 Advanced Application Template",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "application template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.0",
    "yiisoft/yii2": "~2.0.36",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
    "yiisoft/yii2-jui": "*",
    "zxbodya/yii2-tinymce": "*@dev",
    "zxbodya/yii2-elfinder": "*@dev",
    "mihaildev/yii2-ckeditor": "*",
    "mihaildev/yii2-elfinder": "*",
    "kartik-v/yii2-widget-fileinput": "*",
    "kartik-v/yii2-widget-datepicker": "*",
    "kartik-v/yii2-field-range": "dev-master",
    "juratitov/yii2-morrisjs-widget": "dev-master",
    "kartik-v/yii2-date-range": "dev-master",
    "kartik-v/yii2-widget-select2": "dev-master",
    "dkeeper/yii2-mobiledetect": "*",
    "bupy7/yii2-widget-cropbox": "3.*",
    "skeeks/yii2-assets-auto-compress": "*",
    "yidas/yii2-bower-asset": "*",
    "phpoffice/phpspreadsheet": "~1.15.0"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.1.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "symfony/browser-kit": ">=2.7 <=4.2.4"
  },
  "config": {
    "process-timeout": 1800,
    "platform-check": false
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "autoload": {
    "psr-4": {
      "sammaye\\solr\\": ""
    }
  }
}
